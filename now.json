{
  "version": 2,
  "builds": [
    { "src": "api/src/index.js", "use": "@now/node" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "api/src/index.js" }
  ],
  "build": {
    "env": {
      "GRAPHQL_URI": "@graphql_uri",
      "GRAPHQL_LISTEN_PORT": "@graphql_listen_port",
      "COVID_GRAPH_NEO4J_URI": "@covid_graphql_neo4j_uri",
      "COVID_GRAPH_NEO4J_USER": "@covid_graphql_neo4j_user",
      "COVID_GRAPH_NEO4J_PASSWORD": "@covid_graphql_neo4j_password"      
    }
  },
  "env": {
    "GRAPHQL_URI": "@graphql_uri",
    "GRAPHQL_LISTEN_PORT": "@graphql_listen_port",
    "COVID_GRAPH_NEO4J_URI": "@covid_graphql_neo4j_uri",
    "COVID_GRAPH_NEO4J_USER": "@covid_graphql_neo4j_user",
    "COVID_GRAPH_NEO4J_PASSWORD": "@covid_graphql_neo4j_password"
  }
}
